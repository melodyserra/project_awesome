{{#each reviews}}
{{!-- <div class="userImage"> --}}
{{!-- <img src="image"> --}}
	{{!-- </div> --}}
	{{!-- remember to change description to review --}}
<div class="review">
	{{description}}
</div>
{{/each}}
<br>
<%= simple_form_for(@review, remote: true) do |f| %>
  <% if @review.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@review.errors.count, "error") %> prohibited this review from being saved:</h2>

      <ul>
      <% @review.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class='field'>
    <%= f.label :review %><br>
    <%= f.text_field :review %>
  </div>
    <div class="actions">
    <%= f.submit %>
  </div>
  <% end %>